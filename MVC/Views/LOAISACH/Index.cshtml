@model IEnumerable<DataBase.LOAISACHModel>

@{
    ViewBag.Title = "Index";
}
<br />

<script src="~/Scripts/alertify.min.js"></script>
<p>
    <a href="@Url.Action("AddOrEdit","LOAISACH")" class="btn btn-default">Create New</a>
</p>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaLoaiSach)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenLoaiSach)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaLoaiSach)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenLoaiSach)
        </td>
        <td>
            <a href="@Url.Action("AddOrEdit","LOAISACH", new { id = item.MaLoaiSach})" class="btn btn-default">Edit</a> |
            <a href="@Url.Action("Delete","LOAISACH", new { id = item.MaLoaiSach})" class="btn btn-default">Delete</a>
        </td>
    </tr>
}
</table>

